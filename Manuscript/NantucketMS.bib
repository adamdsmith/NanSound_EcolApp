  volume = {5},
  copyright = {\textcopyright{} 2014 The Authors.           Methods in Ecology and Evolution published by John Wiley \& Sons Ltd on behalf of British Ecological Society},
  issn = {2041-210X},
  shorttitle = {Improving Species Distribution Models},
  doi = {10.1111/2041-210X.12184},
  abstract = {Species distribution models (SDMs) are important tools for forecasting the potential impacts of future environmental changes but debate remains over the most robust modelling approaches for making projections. Suggested improvements in SDMs vary from algorithmic development through to more mechanistic modelling approaches. Here, we focus on the improvements that can be gained by conditioning SDMs on more detailed data. Specifically, we use breeding bird data from across Europe to compare the relative performances of SDMs trained on presence\textendash{}absence data and those trained on abundance data. Species distribution models trained on presence\textendash{}absence data, with a poor to slight fit according to Cohen's kappa, show an average improvement in model performance of 0$\cdot$32 (SE $\pm$ 0$\cdot$12) when trained on abundance data. Even those species for which models trained on presence\textendash{}absence data are classified as good to excellent show a mean improvement in Cohen's kappa score of 0$\cdot$05 (SE $\pm$ 0$\cdot$01) when corresponding SDMs are trained on abundance data. This improved explanatory power is most pronounced for species of high prevalence. Our results illustrate that even using coarse scale abundance data, large improvements in our ability to predict species distributions can be achieved. Furthermore, predictions from abundance models provide a greater depth of information with regard to population dynamics than their presence\textendash{}absence model counterparts. Currently, despite the existence of a wide variety of abundance data sets, species distribution modellers continue to rely almost exclusively on presence\textendash{}absence data to train and test SDMs. Given our findings, we advocate that, where available, abundance data rather than presence\textendash{}absence data can be used to more accurately predict the ecological consequences of environmental change. Additionally, our findings highlight the importance of informative baseline data sets. We therefore recommend the move towards increased collection of abundance data, even if only coarse numerical scales of recording are possible.},
  language = {en},
  number = {6},
  journal = {Methods in Ecology and Evolution},
  author = {Howard, Christine and Stephens, Philip A. and Pearce-Higgins, James W. and Gregory, Richard D. and Willis, Stephen G.},
  month = jun,
  year = {2014},
  keywords = {model performance,ordinal abundance data,presence–absence data,random forests,species distribution modelling},
  pages = {506-513},
  file = {C:\\Users\\adsmith\\Zotero\\storage\\HF4V7U4A\\Howard et al. - 2014 - Improving species distribution models the value o.pdf;C:\\Users\\adsmith\\Zotero\\storage\\LWSBLRPZ\\2041-210X.html}
}

@article{JohnstonEtAl2015,
