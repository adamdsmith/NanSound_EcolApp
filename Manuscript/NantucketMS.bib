@article{LegendreEtAl1997,
  title = {Spatial Structure of Bivalves in a Sandflat:: {{Scale}} and Generating Processes},
  volume = {216},
  issn = {0022-0981},
  shorttitle = {Spatial Structure of Bivalves in a Sandflat},
  doi = {10.1016/S0022-0981(97)00092-0},
  abstract = {A survey was conducted during the summer of 1994 within a fairly homogeneous 12.5 ha area of sandflat off Wiroa Island, in Manukau Harbour, New Zealand, to identify factors controlling the spatial distributions of the two dominant bivalves, Macomona liliana Iredale and Austrovenus stutchburyi (Gray), and to look for evidence of adult\textendash{}juvenile interactions within and between species. Most of the large\textendash{}scale spatial structure detected in the bivalve count variables (two species, several size classes of each) was explained by the physical and biological variables. The results of principal component analysis and spatial regression modelling suggest that different factors are controlling the spatial distributions of adults and juveniles. Larger size classes of both species displayed significant spatial structure, with physical variables explaining some but not all of this variation. Smaller organisms were less strongly spatially structured, with virtually all of the structure explained by physical variables. The physical variables important in the regression models differed among size classes of a species and between species. Extreme size classes (largest and smallest) were best explained by the models; physical variables explained from 10\% to about 70\% of the variation across the study site. Significant residual spatial variability was detected in the larger bivalves at the scale of the study site. The unexplained variability (20 to 90\%) found in the models is likely to correspond to phenomena operating at smaller scales. Finally, we found no support for adult\textendash{}juvenile interactions at the scale of our study site, given our sampling scale, after controlling for the effects of the available physical variables. This is in contrast to significant adult\textendash{}juvenile interactions found in smaller\textendash{}scale surveys and in field experiments. Our perception of adult\textendash{}juvenile interactions thus depends on the scale of study.},
  number = {1},
  journal = {Journal of Experimental Marine Biology and Ecology},
  author = {Legendre, P. and Thrush, S. F. and Cummings, V. J. and Dayton, P. K. and Grant, J. and Hewitt, J. E. and Hines, A. H. and McArdle, B. H. and Pridmore, R. D. and Schneider, D. C. and Turner, S. J. and Whitlatch, R. B. and Wilkinson, M. R.},
  month = sep,
  year = {1997},
  keywords = {Adult–juvenile interaction,Autocorrelograms,Bivalves,Spatial modelling,Spatial structure},
  pages = {99-128},
  file = {C:\\Users\\adsmith\\Zotero\\storage\\D9CWEXX3\\Legendre et al. - 1997 - Spatial structure of bivalves in a sandflat Scal.pdf;C:\\Users\\adsmith\\Zotero\\storage\\FLU6B76I\\S0022098197000920.html}
}

